{

  "manifest_version": 2,
  "name": "Weeaboo(ä»®)",
  "version": "0.1",

  "description": "A tool for unblocking TVer.jp(tv-asahi,fujitv,ntv,tbs,tv-tokyo),abema.tv,dmm.co.jp geolocation restriction.",

  "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "*://*.tver.jp/*",
      "*://*.api.brightcove.com/*",
      "*://ad.auditude.com/*",
      "*://pubads.g.doubleclick.net/*",
      "*://*.stream.ne.jp/*",
      "*://*.fod.fujitv.co.jp/*",
      "*://*.abema.tv/*",
      "*://*.akamaized.net/*",
      "*://api.abema.io/*",
      "*://www.tv-asahi.co.jp/*",
      "*://*.dmm.co.jp/*"
  ],
  "background": {
    "scripts": ["iplib.js","background.js"]

  },
  "browser_action": {
    "default_popup": "popup.html"
  }

}
